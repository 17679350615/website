activeIndex: '1',\r\n\r\n    }\r\n  },\r\n\r\n\r\n\r\n  created() {\r\n\r\n    this.judge();\r\n\r\n  },\r\n\r\n  methods: {\r\n\r\n    judge() {\r\n      let num = this.$route.query.number;\r\n      if(num==0) {\r\n        this.findAllJournalisms();\r\n      }\r\n      else if(num==1) {\r\n        this.findAllEvangels()\r\n      }\r\n      else if(num==2) {\r\n        this.findAllSpecials()\r\n      }\r\n      else if(num==3) {\r\n        this.findAllSciences()\r\n      }\r\n      else if(num==4) {\r\n        this.findAllSubjects()\r\n      }\r\n      else if(num==5) {\r\n        this.findAllMessages()\r\n      }\r\n    },\r\n    findAllJournalisms() {\r\n      this.$axios.get(\"/journalism/findAllJournalisms\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    findAllEvangels() {\r\n      this.$axios.get(\"/evangel/findAllEvangels\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    findAllSpecials() {\r\n      this.$axios.get(\"/special/findAllSpecials\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    findAllSciences() {\r\n      this.$axios.get(\"/science/findAllSciences\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    findAllSubjects() {\r\n      this.$axios.get(\"/subject/findAllSubjects\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    findAllMessages() {\r\n      this.$axios.get(\"/message/findAllMessages\")\r\n          .then((res) => {\r\n            this.datas = res.data;\r\n            console.log(res.data);\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    handleSelect() {\r\n\r\n    },\r\n    login() {\r\n      this.$router.replace(\"/login\")\r\n    },\r\n    toList(number) {\r\n      this.$router.push({path:'/list',query:{number:number}})\r\n    },\r\n    toDetail(data) {\r\n      this.$router.push({path: '/detail', query:{title:data.title,date:data.date,content:data.content}});\r\n    }\r\n\r\n  },\r\n\r\n  watch: { //通过watch来监听路由变化\r\n    \"$route\": function () {\r\n      this.judge();\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views"}]}