AA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,MAAA,CAAA,QAAA,CAAA;AACA,UAAA,IAAA,IAAA,SAAA,EAAA,OAAA,EAAA;AACA,UAAA,EAAA,GAAA,IAAA,IAAA,CAAA,IAAA,CAAA;AACA,aAAA,EAAA,CAAA,WAAA,KAAA,GAAA,IAAA,EAAA,CAAA,QAAA,KAAA,CAAA,IAAA,GAAA,GAAA,EAAA,CAAA,OAAA,EAAA;AACA;AA/IA;AAjDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin-top:50px;\">\r\n      <el-input v-model=\"evangelTitle\" placeholder=\"请输入新闻标题\" style=\"width:80%\" clearable></el-input>\r\n      <el-button type=\"primary\" style=\"margin-left: 10px;\" @click=\"searchEvangelByTitle\">搜索</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left:10px;margin-bottom: 20px;\" @click=\"showDialog\">新增新闻</el-button>\r\n    </div>\r\n    <div>\r\n      <el-table\r\n          @selection-change=\"handleSelectionChange\"\r\n          :data=\"evangels\"\r\n          border>\r\n        <el-table-column\r\n            type=\"selection\"\r\n            width=\"55\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"uid\"\r\n            label=\"编号\"\r\n            width=\"50\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"title\"\r\n            label=\"标题\"\r\n            width=\"300\"\r\n            :show-overflow-tooltip=\"true\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"date\"\r\n            label=\"发布日期\"\r\n            :formatter= \"dateformat\"\r\n            width=\"120\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"content\"\r\n            label=\"内容\"\r\n            width=\"380\"\r\n            :show-overflow-tooltip=\"true\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            prop=\"http\"\r\n            label=\"Http\"\r\n            width=\"200\"\r\n            :show-overflow-tooltip=\"true\">\r\n        </el-table-column>\r\n        <el-table-column\r\n            fixed= \"right\"\r\n            min-width=\"150%\"\r\n            label=\"操作\">\r\n          align=\"left\">\r\n          <template slot-scope=\"scope\">\r\n            <el-button\r\n                size=\"small\"\r\n                @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n            <el-button\r\n                size=\"small\"\r\n                type=\"danger\"\r\n                @click=\"handleDelete(scope.row)\">删除</el-button>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      <el-button type=\"danger\" size=\"mini\" style=\"margin-top: 12px\" :disabled=\"multipleSelection.length === 0\" @click=\"deleteEvangels\">批量删除</el-button>\r\n    </div>\r\n    <el-dialog :title=\"dialogTitle[dialogStatus]\" :visible.sync=\"dialogFormVisible\" center>\r\n      <el-form :model=\"form\" :rules=\"rules\" ref=\"evangel\">\r\n        <el-form-item label=\"标题\" :label-width=\"formLabelWidth\" prop=\"title\" >\r\n          <el-input v-model=\"form.title\" type=\"textarea\" maxlength=\"30\" autocomplete=\"off\" show-word-limit></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"入职时间\" :label-width=\"formLabelWidth\">\r\n          <el-date-picker type=\"date\"  placeholder=\"选择日期\" v-model=\"form.date\" value-format= \"yyyy-MM-dd\" style=\"width: 100%;\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\" :label-width=\"formLabelWidth\" prop=\"introduce\">\r\n          <el-input\r\n              type=\"textarea\"\r\n              v-model=\"form.content\"\r\n              maxlength=\"500\"\r\n              show-word-limit\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"Http\" :label-width=\"formLabelWidth\" prop=\"http\" >\r\n          <el-input v-model=\"form.http\" type=\"textarea\" maxlength=\"30\" autocomplete=\"off\" show-word-limit></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"submitEvangel\">确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n    <el-upload\r\n        style=\"margin-top:20px;\"\r\n        action=\"/upload\"\r\n        multiple\r\n        :on-success=\"handleSuccess\"\r\n        :limit=\"3\"\r\n        :file-list=\"fileList\">\r\n      <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n    </el-upload>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// @ is an alias to /src\r\n\r\n\r\nexport default {\r\n  name: 'Home',\r\n  components: {\r\n  },\r\n  data () {\r\n    return {\r\n      fileList: [],\r\n      evangelTitle: '',\r\n      multipleSelection:[],\r\n      dialogTitle: {\r\n        addEvangel: \"新增新闻\",\r\n        editEvangel: \"编辑新闻\"\r\n      },\r\n      dialogStatus: \"\",\r\n      rules: {\r\n        title: [\r\n          { required: true, message: '请输入标题', trigger: 'blur' }\r\n        ],\r\n        date: [\r\n          { required: true, message: '请选择时间', trigger: 'change' }\r\n        ],\r\n        content: [\r\n          { required: true, message: '请输入内容'},\r\n        ],\r\n        http: [\r\n          { required: true, message: '请输入网址'}\r\n        ]\r\n      },\r\n      msg: '',\r\n      evangels:[],\r\n      search: '',\r\n      dialogFormVisible: false,\r\n      form: {\r\n        title: '',\r\n        date: '',\r\n        content: '',\r\n        http: ''\r\n      },\r\n      formLabelWidth: '120px',\r\n    }\r\n  },\r\n  created() {\r\n    this.findAllEvangels();\r\n  },\r\n  watch:{\r\n    evangelTitle () {\r\n      this.findAllEvangels();\r\n    }\r\n  },\r\n  methods: {\r\n    handleSuccess (file) {\r\n      const filename = file.name;\r\n      this.$message({\r\n        message: '文件上传成功！',\r\n        type: 'success'\r\n      });\r\n    },\r\n    searchEvangelByTitle () {\r\n      if (this.evangelTitle !== '') {\r\n        this.$axios.get(\"/evangel/findEvangelByTitle?title=\"+this.evangelTitle).then( (resp)=> {\r\n          this.evangels = resp.data;\r\n        }).catch( (error)=>{\r\n          this.$message({\r\n            type: 'error',\r\n            message: \"查询失败，原因是\"+error.data.message\r\n          });\r\n        })\r\n      }\r\n    },\r\n    deleteEvangels () {\r\n      this.$confirm('此操作将永久删除新闻, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        let ids = '?';\r\n        this.multipleSelection.forEach( (item)=>{\r\n          ids += 'ids='+item.uid+'&'\r\n        });\r\n        this.$axios.post(\"/evangel/deleteEvangelsByIds\"+ids).then( (resp)=> {\r\n          if (resp) {\r\n            this.findAllEvangels();\r\n            this.$message.success(\"删除成功！\");\r\n          }\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });\r\n      });\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.multipleSelection = val;\r\n      console.log(val)\r\n    },\r\n    submitEvangel () {\r\n      if (this.dialogStatus === 'addEvangel'){\r\n        this.addEvangel();\r\n      } else if(this.dialogStatus === \"editEvangel\") {\r\n        this.editEvangel();\r\n      }\r\n    },\r\n    findAllEvangels () {\r\n      this.$axios.get(\"/evangel/findAllEvangels\")\r\n          .then((res) => {\r\n            this.evangels = res.data;\r\n\r\n          })\r\n          .catch(error => {\r\n            this.$message({\r\n              type: 'error',\r\n              message: \"查询失败，原因是\"+error.data.message\r\n            });\r\n          });\r\n    },\r\n    handleEdit (row) {\r\n      this.dialogFormVisible = true;\r\n      this.dialogStatus = \"editEvangel\";\r\n      this.form.uid = row.uid;\r\n      this.form.title= row.title;\r\n      this.form.date = row.date;\r\n      this.form.content = row.content;\r\n      this.form.http = row.http;\r\n    },\r\n    handleDelete (row) {\r\n      this.$confirm('此操作将永久删除该新闻, 是否继续?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$axios.post(\"/evangel/deleteEvangel?uid=\"+row.uid).then ((resp)=>{\r\n          this.$message.success(\"删除成功!\")\r\n          this.findAllEvangels();\r\n        })\r\n      }).catch(() => {\r\n        this.$message({\r\n          type: 'info',\r\n          message: '已取消删除'\r\n        });\r\n      });\r\n    },\r\n    showDialog () {\r\n      this.form.uid = '';\r\n      this.form.title= '';\r\n      this.form.date = '';\r\n      this.form.content = '';\r\n      this.form.http = '';\r\n      this.dialogFormVisible = true;\r\n      this.dialogStatus = \"addEvangel\";\r\n    },\r\n    addEvangel () {\r\n      this.$refs.evangel.validate((valid) => {\r\n        if (valid) {\r\n          this.$axios.post(\"/evangel/addEvangel\",this.form).then( (resp)=>{\r\n            if (resp) {\r\n              this.dialogFormVisible = false;\r\n              this.$message.success(\"添加成功！\");\r\n              this.findAllEvangels();\r\n            }\r\n          }).catch((error)=>{\r\n            this.$message({\r\n              message: '添加数据失败，原因是'+error.data.message,\r\n              type: 'error'\r\n            })\r\n          })\r\n        } else {\r\n          this.$message({\r\n            message: '请输入所有字段',\r\n            type: 'error'\r\n          })\r\n          return false;\r\n        }\r\n      });\r\n    },\r\n    editEvangel () {\r\n      this.$axios.post(\"/evangel/updateEvangel\",this.form).then( (resp)=>{\r\n        this.dialogFormVisible = false;\r\n        this.$message.success(\"修改成功!\");\r\n        this.findAllEvangels();\r\n      }).catch( (error)=> {\r\n        this.$message({\r\n          message: '数据更新失败，原因是'+error.data.message,\r\n          type: 'error'\r\n        })\r\n      })\r\n    },\r\n    dateformat:function(row, column) {\r\n      let date = row[column.property];\r\n      if (date == undefined) return '';\r\n      let dt = new Date(date);\r\n      return dt.getFullYear() + '-' + (dt.getMonth() + 1) + '-' + dt.getDate()\r\n    },\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.table {\r\n  margin: 0 auto;\r\n  clear: both;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}